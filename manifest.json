{
  "manifest_version": 2,
  "name": "Media Carousel",
  "version": "0.1.0",
  "description": "Collects all embedded and linked media of the current page and displays this content in a carousel.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/64.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "inject-overlay.js",
    "media-carousel.html",
    "media-carousel.js"
  ]
}
