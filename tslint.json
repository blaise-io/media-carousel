{
  "defaultSeverity": "warn",
  "extends": ["tslint:latest"],
  "rules": {
    "interface-name": false,
    "no-console": [true, "log"],
    "object-literal-sort-keys": false,
    "trailing-comma": [  true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  }
}
